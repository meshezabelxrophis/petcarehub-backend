{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/generate-ai-response.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/create-payment-intent.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/store-ai-output.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/generate-ai-response",
      "dest": "/api/generate-ai-response.js"
    },
    {
      "src": "/api/create-payment-intent",
      "dest": "/api/create-payment-intent.js"
    },
    {
      "src": "/api/store-ai-output",
      "dest": "/api/store-ai-output.js"
    }
  ],
  "installCommand": "cd api && npm install"
}



