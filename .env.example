# Firebase Configuration
# Copy this file to .env and fill in your actual Firebase values
# Get these values from: https://console.firebase.google.com/project/fyppp-5b4f0/settings/general

REACT_APP_FIREBASE_API_KEY=your_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id
REACT_APP_FIREBASE_DATABASE_URL=https://your-project-default-rtdb.firebaseio.com

# Development Settings
NODE_ENV=production
REACT_APP_USE_FIREBASE_EMULATORS=false

# To use Firebase emulators in development, set to true
# REACT_APP_USE_FIREBASE_EMULATORS=true
